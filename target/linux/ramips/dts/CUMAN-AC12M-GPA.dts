/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "cuman,ac12m-gpa","mediatek,mt7621-soc";
	model = "Cuman-AC12M-GPA";


	aliases {
		led-boot = &sn3218_led16;
		led-failsafe = &sn3218_led16;
		led-running = &sn3218_led16;
		led-upgrade = &sn3218_led16;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	palmbus: palmbus@1E000000 {
		i2c@900 {
			status = "okay";

			/*######################################*/
			/* reg : number of LED line (could be from 0 to 17) */
			leds: sn3218@54 {
				compatible = "si-en,sn3218";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x54>;

				sn3218_led00: led00@0 {
					label = "sn3218_led_reg_00";
					reg = <0x0>;
				};
				sn3218_led01: led01@1 {
					label = "sn3218_led_reg_01";
					reg = <0x1>;
				};
				sn3218_led02: led02@2 {
					label = "sn3218_led_reg_02";
					reg = <0x2>;
				};
				sn3218_led03: led03@3 {
					label = "sn3218_led_reg_03";
					reg = <0x3>;
				};
				sn3218_led04: led04@4 {
					label = "sn3218_led_reg_04";
					reg = <0x4>;
				};
				sn3218_led05: led05@5 {
					label = "sn3218_led_reg_05";
					reg = <0x5>;
				};
				sn3218_led06: led106@6 {
					label = "sn3218_led_reg_06";
					reg = <0x6>;
				};
				sn3218_led07: led07@7 {
					label = "sn3218_led_reg_07";
					reg = <0x7>;
				};
				sn3218_led08: led08@8 {
					label = "sn3218_led_reg_08";
					reg = <0x8>;
				};
				sn3218_led09: led09@9 {
					label = "sn3218_led_reg_09";
					reg = <0x9>;
				};
				sn3218_led10: led10@10 {
					label = "sn3218_led_reg_10";
					reg = <0xA>;
				};
				sn3218_led11: led11@11 {
					label = "sn3218_led_reg_11";
					reg = <0xB>;
				};
				sn3218_led12: led12@12 {
					label = "sn3218_led_reg_12";
					reg = <0xC>;
				};
				sn3218_led13: led13@13 {
					label = "sn3218_led_reg_13";
					reg = <0xD>;
				};
				sn3218_led14: led14@14 {
					label = "sn3218_led_reg_14";
					reg = <0xE>;
				};
				sn3218_led15: led15@15 {
					label = "sn3218_led_reg_15";
					reg = <0xF>;
				};
				sn3218_led16: led16@16 {
					label = "sn3218_led_reg_16";
					reg = <0x10>;
				};
				sn3218_led17: led17@17 {
					label = "sn3218_led_reg_17";
					reg = <0x11>;
				};

			};
			/*######################################*/
		};
	};

	sn3218_activator {
		compatible = "gpio-leds";
		sn3218_on_pin {
			label = "sn3218_on_pin";
			gpios = <&gpio0 5 GPIO_ACTIVE_HIGH>;
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		wps {
			label = "wps";
			gpios = <&gpio0 18 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};
	};


};

&sdhci {
	status = "disabled";
};

&spi0 {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			firmware: partition@50000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x50000 0xfb0000>;
			};
		};
	};
};

&pcie {
	status = "okay";
};

&pcie0 {
	wifi0: wifi@0,0 {
		compatible = "pci14c3,7603";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x0000>;
		mtd-mac-address = <&factory 0xe000>;
		mtd-mac-address-increment = <3>;
	};
};

&pcie1 {
	wifi1: wifi@0,0 {
		compatible = "pci14c3,7662";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x8000>;
		ieee80211-freq-limit = <5000000 6000000>;
		mtd-mac-address = <&factory 0xe000>;
		mtd-mac-address-increment = <4>;
		led {
			led-sources = <2>;
		};
	};
};

&ethernet {
	mtd-mac-address = <&factory 0xe000>;
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "wdt", "rgmii2", "jtag", "mdio";
			ralink,function = "gpio";
		};
	};
};
